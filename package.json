{
  "name": "xandeum-analytics",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:api": "tsx render-api-server.ts",
    "dev:realtime": "tsx realtime-server.ts",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "build": "next build",
    "start": "next start",
    "start:realtime": "tsx realtime-server.ts",
    "lint": "next lint",
    "render:server": "tsx render-server.ts",
    "render:api-server": "tsx render-api-server.ts",
    "render:realtime": "tsx realtime-server.ts",
    "clean": "rm -rf .next out build .cache .turbo .vercel tsconfig.tsbuildinfo && find . -name '*.tsbuildinfo' -type f -delete 2>/dev/null || true",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "clean:heavy": "bash clean-heavy.sh",
    "clean:everything": "npm run clean && npm run clean:heavy",
    "reset": "npm run clean:all && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Analytics platform for Xandeum pNodes",
